{
  "kind": "collectionType",
  "collectionName": "use_cases",
  "info": {
    "singularName": "use-case",
    "pluralName": "use-cases",
    "displayName": "UseCase",
    "description": "Categorizes bots by purpose (growth, data-extraction, lead-generation, etc.)"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "icon": {
      "type": "string"
    },
    "bots": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::bot.bot",
      "mappedBy": "useCases"
    }
  }
}
